        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="Ultimate HAM"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.tylerburnham42.ultimateHAMPrep.MainActivity"
